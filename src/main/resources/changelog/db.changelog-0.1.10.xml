<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="tibank2071@gmail.com" id="changelog-0.1.10" >
        <comment>"create foregin key in table orderdetails"</comment>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="orderdetails" constraintName="FK_ORDERDETAILS_ORDER"
                                 referencedColumnNames="id" referencedTableName="orders"
                                 onDelete="RESTRICT" onUpdate="RESTRICT"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="orderdetails" constraintName="FK_PRODUCT_ORDERDETAILS"
                                 referencedColumnNames="id" referencedTableName="products"
                                 onDelete="RESTRICT" onUpdate="RESTRICT"/>
    </changeSet>
</databaseChangeLog>